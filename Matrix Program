#include<stdio.h>
int main()
{
	int a[3] [3],i,j;
	printf("Enter Matrix Elements : \n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("Enter the elements[%d][%d]",i,j);
			scanf("%d",&a[i][j]);
		}
	}
	printf("Matrix is\n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%3d",a[i][j]);
		}
	}
	getch();
}
